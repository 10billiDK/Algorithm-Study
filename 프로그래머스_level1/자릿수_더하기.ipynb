    "#자연수 N이 주어지면, N의 각 자릿수의 합을 구해서 return\n",
    "def solution(n):\n",
    "    answer=0\n",
    "    n=str(n)\n",
    "    n=list(n)\n",
    "    for i in n:\n",
    "        answer += int(i)\n",
    "    return answer"
