    "def solution(a, b):\n",
    "    answer = 0\n",
    "    \n",
    "    if a<=b:\n",
    "        for i in range(a,b+1):\n",
    "            answer += i\n",
    "    elif a>b:\n",
    "        temp=b\n",
    "        b=a\n",
    "        a=temp\n",
    "        for i in range(a,b+1):\n",
    "            answer += i\n",
    "    return answer"
