    "def solution(a, b):\n",
    "    answer = 0\n",
    "    for i in range(len(a)):\n",
    "        for j in range(len(b)):\n",
    "            if i==j:\n",
    "                answer += a[i]*b[j]\n",
    "            \n",
    "    return answer"
